{
  "version": 2,
  "builds": [
    {
      "src": "api/create-account.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/get-profile.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/update-password.js",
      "use": "@vercel/node"
    },
     {
      "src": "/api/reset-password.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/create-account",
      "dest": "api/create-account.js",
      "timeout": 60
    },
    {
      "src": "/api/login",
      "dest": "api/login.js",
      "timeout": 60
    },
    {
      "src": "/api/get-profile",
      "dest": "api/get-profile.js",
      "timeout": 60
    },
    {
      "src": "/api/update-password",
      "dest": "api/update-password.js",
      "timeout": 60
    },
     {
      "src": "/api/reset-password",
      "dest": "api/reset-password.js",
      "timeout": 60
    }
  ]
}
