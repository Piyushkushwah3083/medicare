{
  "version": 2,
  "builds": [
    {
      "src": "api/create-account.js",
      "use": "@vercel/node",
      "config": { "timeout": 30 }
    },
    {
      "src": "api/login.js",
      "use": "@vercel/node",
      "config": { "timeout": 30 }
    }
  ],
  "routes": [
    { "src": "/api/create-account", "dest": "api/create-account.js" },
    { "src": "/api/login", "dest": "api/login.js" }
  ]
}
