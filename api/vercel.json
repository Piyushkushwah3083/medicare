{
  "version": 2,
  "builds": [
    {
      "src": "api/create-account.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/login.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/create-account",
      "dest": "api/create-account.js",
      "timeout": 30
    },
    {
      "src": "/api/login",
      "dest": "api/login.js",
      "timeout": 30
    }
  ]
}
