{
  "version": 2,
  "builds": [
    {
      "src": "api/signup.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/login.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/signup",
      "dest": "api/signup.js"
    },
    {
      "src": "/api/login",
      "dest": "api/login.js"
    }
  ],
  "functions": {
    "api/signup": { "timeout": 30 },
    "api/login": { "timeout": 30 }
  }
}
